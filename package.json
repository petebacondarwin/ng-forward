    "angular": "^1.4.0",
    "angular-mocks": "^1.4.3",
    "babel": "^5.0.8",
    "babel-eslint": "^3.1.23",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "chai": "^2.2.0",
    "derequire": "^2.0.2",
    "eslint": "^0.24.0",
    "gulp": "^3.8.11",
    "gulp-babel": "^5.0.0",
    "karma": "^0.13.3",
    "karma-angular": "0.0.6",
